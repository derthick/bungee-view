<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN"
    "http://java.sun.com/j2ee/dtds/web-app_2_2.dtd">
    <web-app>
      <display-name>Bungee View</display-name>
        <servlet>
            <servlet-name>Bungee</servlet-name>
            <servlet-class>edu.cmu.cs.bungee.servlet.Servlet</servlet-class>
            <init-param>
                <param-name> server </param-name>
                <param-value> jdbc:mysql://localhost/ </param-value>
            </init-param>
            <init-param>
                <param-name> user </param-name>
                <param-value> bungee </param-value>
            </init-param>
            <init-param>
                <param-name> pwd </param-name>
                <param-value> p5pass </param-value>
            </init-param>
            <init-param>
                <param-name> dbs </param-name>
                <param-value>famsf,chipapers,HistoryMakers,loc,ChartresVezelay,Visuals,Movie,personal,music,wpa,hp3,cm</param-value>
            </init-param>
        </servlet>
    <servlet-mapping>
        <servlet-name>Bungee</servlet-name>
        <url-pattern>/Bungee</url-pattern>
    </servlet-mapping>
    </web-app>
    
